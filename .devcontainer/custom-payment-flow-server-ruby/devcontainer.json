{
  "name": "custom-payment-flow/server/ruby",
  "dockerComposeFile": [
    "docker-compose.yml",
    "docker-compose.override.yml"
  ],
  "service": "web",
  "workspaceFolder": "/work",
  "hostRequirements": {
    "cpus": 2,
    "memory": "4gb"
  },
  "forwardPorts": [4242],
  "portsAttributes": {
    "4242": {
      "label": "web",
      "onAutoForward": "notify"
    },
  },
  "otherPortsAttributes": {
    "onAutoForward": "silent"
  }
}

