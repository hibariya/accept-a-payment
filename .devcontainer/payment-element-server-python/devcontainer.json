{
  "name": "payment-element/server/python",
  "dockerComposeFile": [
    "docker-compose.yml",
    "docker-compose.override.yml"
  ],
  "service": "web",
  "workspaceFolder": "/work/payment-element/server/python",
  "hostRequirements": {
    "cpus": 1,
    "memory": "1gb"
  },
  "forwardPorts": [4242],
  "portsAttributes": {
    "4242": {
      "label": "web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "silent"
  }
}

