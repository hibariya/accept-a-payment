{
  "name": "${WORKING_DIR}",
  "dockerComposeFile": [
    "docker-compose.yml",
    "docker-compose.override.yml"
  ],
  "service": "${SERVICE}",
  "runServices": ${RUN_SERVICES},
  "workspaceFolder": "/work/${WORKING_DIR}",
  "hostRequirements": {
    "cpus": 1,
    "memory": "1gb"
  },
  "forwardPorts": [${PORT}],
  "portsAttributes": {
    "${PORT}": {
      "label": "web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "silent"
  }
}

